{% extends "::base.html.twig"%}
{% form_theme form 'BoAnnonceBundle:Form:fields.html.twig' %}

{% block body %}
    <div class='container signup'>
        <form action="{{ path('_create')}}" method="post" {{ form_enctype(form) }}>
            {{ form_widget(form) }}
            <button class="btn btn-lg btn-primary btn-block" type="submit">Sign up</button>
        </form>
    </div>
{% endblock %}